	.menu
		position relative
		width 74px
		height 100%
		display -webkit-flex
		display -moz-flex
		display -ms-flex
		display -o-flex
		display flex
		align-items center
		justify-content center
		&.active
			span
				transform rotate(90deg)
				&:after
					top 0
					transform rotate(-90deg)
					transition top 0.3s,transform 0.3s
				&:before
					bottom 0
					transform rotate(-90deg)
					transition bottom 0.3s, transform 0.3s
		span
			position relative
			display block
			width 35px
			height 4px
			background #666
			border-radius 2px
			transition transform 0.3s
			&:after
				content ""
				position absolute
				top -15px
				left 0
				right 0
				width 100%
				height 4px
				border-radius 2px
				background #666
				transition top 0.3s,transform 0.3s
			&:before
				content ""
				position absolute
				bottom -15px
				left 0
				right 0
				width 100%
				height 4px
				border-radius 2px
				background #666
				transition bottom 0.3s, transform 0.3s		
		.child-menu
			position absolute
			right 20px
			top 100%
			padding-top 10px
			display none
			.nav
				position relative
				padding 0 20px
				background #fff
				&:after
					content ""
					position absolute
					right 10px
					top -20px
					border 10px solid transparent
					border-bottom-color #fff
			a
				display block
				font-size 24px
				color #666
				text-align center
				width 120px
				height 60px
				line-height 60px
				border-bottom 1px solid #e5e5e5
				
